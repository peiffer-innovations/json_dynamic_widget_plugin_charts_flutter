        "initial_hint_animation"
      ]
    },
    "template": {
      "type": "column",
      "children": [
        {
          "type": "expanded",
          "child": "##template(!{{value}})##"
        },
        {
          "type": "text",
          "args": {
            "maxLines": 1,
            "overflow": "ellipsis",
            "text": "{{title}}"
          }
        }
      ]
    }
  },
  "child": {

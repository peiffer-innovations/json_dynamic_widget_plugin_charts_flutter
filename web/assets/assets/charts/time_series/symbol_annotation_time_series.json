        "tablet": "##create_series(type:DateTime, items:{{data;$.tablet}}, name:Tablet, domain:$.timeCurrent, measure:$.sales, color:!{{charts.MaterialPalette.green}})##",
        "annotationSeries1": "##create_series(boundsLineRadiusPxKey:3.5, rendererIdKey:customSymbolAnnotation, type:DateTime, items:{{data;$.annotation1}}, name:Annotation Series 1, domain:$.timeCurrent, domainLowerBound:$.timePrevious, domainUpperBound:$.timeTarget, color:!{{charts.MaterialPalette.gray}})##",

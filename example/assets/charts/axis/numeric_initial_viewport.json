  "child": {
    "type": "set_value",
    "args": {
      "series": {
        "global": "##create_series(type:int, items:{{data;$.global}}, name:Global, domain:$.year, measure:$.sales)##"
      }
    },
    "child": {
      "type": "line_chart",
      "args": {
        "animate": false,
        "behaviors": [
          {
            "type": "pan_and_zoom_behavior"
          }
        ],
        "domainAxis": {
          "type": "numeric",
          "viewport": {
            "min": 3.0,
            "max": 7.0
          }
        },
        "series": "{{series}}"
      }
    }
  }
}